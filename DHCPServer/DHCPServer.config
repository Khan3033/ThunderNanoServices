set (autostart false)
set (preconditions Platform)
set (outofprocess true)

map()
    key(events)
    kv("name" "wpeserver")
    key(servers)
    map()
        kv(interface eth0)
        kv(poolstart 100)
        kv(poolsize 50)
        kv(active false)
    end()
    map()
        kv(interface wlan0)
        kv(poolstart 100)
        kv(poolsize 50)
        kv(active false)
    end()
end()
ans(configuration)
